{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cleanapp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/app/app.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildapp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/app/app.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "cleantests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/Laravel.Tests/Laravel.Tests.msbuildproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildtests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Laravel.Tests/Laravel.Tests.msbuildproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}